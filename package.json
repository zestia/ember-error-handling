{
  "name": "@zestia/ember-error-handling",
  "version": "1.1.4",
  "description": "An Error Handling service for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm run lint:js && npm run lint:hbs",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "prettier": "prettier '**/*.{js,json,html,md}' --write",
    "start": "ember serve",
    "test": "ember test && npm run lint",
    "test:all": "ember try:each",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zestia/ember-error-handling.git"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "author": "Zestia",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "^1.1.0",
    "@zestia/ember-template-lint-plugin": "^2.0.4",
    "@zestia/eslint-config": "^2.0.31",
    "@zestia/prettier-config": "^0.0.6",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-cli": "~3.14.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-fetch": "^6.7.2",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^5.3.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-source": "~3.14.1",
    "ember-template-lint": "^1.8.2",
    "ember-try": "^1.3.0",
    "eslint": "^6.6.0",
    "loader.js": "^4.7.0",
    "pretender": "^3.0.4",
    "prettier": "^1.19.1",
    "qunit-dom": "^0.9.2",
    "release-it": "^12.4.3"
  },
  "dependencies": {
    "ember-cli-babel": "^7.11.1",
    "ember-cli-htmlbars": "^4.0.0"
  },
  "keywords": [
    "ember-addon",
    "error",
    "handling"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://zestia.github.io/ember-error-handling"
  },
  "bugs": {
    "url": "https://github.com/zestia/ember-error-handling/issues"
  },
  "homepage": "https://github.com/zestia/ember-error-handling",
  "configPath": "tests/dummy/config"
}
